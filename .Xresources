rofi.location:          0
rofi.color-enabled:     true
!                       'background', 'border'
rofi.color-window:      argb:dc111111, argb:dc111111
! State:                'bg',          'fg',   'bgalt',       'hlbg',        'hlfg'
rofi.color-normal:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ffefa211
rofi.color-urgent:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ffefa211
rofi.color-active:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ffefa211
rofi.separator-style:   dash
rofi.fullscreen:        true
rofi.padding:           130
rofi.hide-scrollbar:    true
rofi.line-margin:       5
rofi.fake-transparency: false
rofi.yoffset:           0
rofi.xoffset:           0
rofi.width:             1366
rofi.opacity:           90
rofi.font:              SourceCodePro-Regular 12


#define base00 #111111
#define base01 #333333
#define base02 #555555
#define base03 #777777
#define base04 #999999
#define base05 #bbbbbb
#define base06 #dddddd
#define base07 #ffffff
#define base08 #ff4136
#define base09 #ff851b
#define base0A #ffdc00
#define base0B #2ecc40
#define base0C #7fdbff
#define base0D #0074d9
#define base0E #b10dc9
#define base0F #85144b


*background:  #03070B
*foreground:  #D7D7D7

! black
*color0:      #14161A
*color8:      #14161A
! red
*color1:      #8C5760
*color9:      #8C5760
! green
*color2:      #7B8C58
*color10:     #7B8C58
! yellow
*color3:      #B28C55
*color11:     #B28C55
! blue
*color4:      #58698C
*color12:     #58698C
! magenta
*color5:      #B23450
*color13:     #B23450
! cyan
*color6:      #737373
*color14:     #737373
! white
*color7:      #D7D7D7
*color15:     #D7D7D7

*cursorColor: #D7D7D7
*colorUL:     #7B8C58

*xterm*faceName: SourceCodePro-Regular:size=10:antialias=true

xterm*VT100.Translations: #override \
                 Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
                 Ctrl Shift <Key>C:    copy-selection(CLIPBOARD)
